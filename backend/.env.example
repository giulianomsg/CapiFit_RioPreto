# Configurações do Servidor
# A porta em que o servidor backend irá rodar
PORT=5000

# Credenciais do Banco de Dados MySQL
# Endereço do seu servidor de banco de dados
DB_HOST="localhost"
# Usuário de acesso ao banco de dados
DB_USER="seu_usuario_mysql"
# Senha de acesso ao banco de dados
DB_PASSWORD="sua_senha_mysql"
# Nome do banco de dados criado
DB_NAME="capifit_db"

# Segredos para Autenticação JWT
# Use um gerador de string aleatória e segura para este campo (ex: https://www.lastpass.com/features/password-generator)
JWT_SECRET="gere_uma_string_aleatoria_e_segura_aqui_com_pelo_menos_32_caracteres"

# Chave da API do Google Gemini (para funcionalidades de IA)
# Obtenha sua chave em https://aistudio.google.com/
API_KEY="SUA_CHAVE_API_GEMINI_AQUI"
